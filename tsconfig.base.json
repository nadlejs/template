{
	"compilerOptions": {
		"lib": ["ESNext"],
		"target": "ES2020",
		"moduleResolution": "NodeNext",
		"module": "NodeNext",
		"allowJs": true,
		"isolatedModules": true,
		"resolveJsonModule": true,
		"declaration": true,
		"sourceMap": true,
		"declarationMap": true,
		"strict": true,
		"skipLibCheck": true,
		"jsx": "react-jsx",
		"stripInternal": true,
		"preserveWatchOutput": true
	}
}
